<p class="text-center form-heading">アカウントを作成</p>
    <div class="form-heading-link text-center">
      または<%= link_to("ログイン",new_user_session_path) %>してください
    </div>  
    <div class="form users-form">
      <div class="form-body">
<%= form_with model: @user, url: registration_path(resource_name), local: true, maltipart:true, method: :post do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <form>
  <div class="field">
    <%= f.label :name %><br>
    <span class="required-item">※必須</span><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
  </div>

  <div class="field">
    <%= f.label :image %><br />
    <%= f.file_field :image %>
    <!--    <input name="image_name" type="file">-->
  </div>

  <div class="field">
    <%= f.label :email %><br>
    <span class="required-item">※必須</span><br />
    <%= f.email_field :email, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password %><br>
    <span class="required-item">※必須 8文字以上、英数字を両方含めてください</span><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br>
    <span class="required-item">※必須</span><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>

  <div class="actions">
    <%= f.submit "新規作成" %>
  </div>
<% end %>

<!--  <%= render "devise/shared/links" %>  -->
      </div>
    </div>
    </form>
