<div class="individual-header text-center">アカウント一覧</div> 
  <div class="content">
      <% @users.each do |user| %>
        <div class="row justify-content-center">
        <div class="card users-index-item">
          <div class="user-info d-flex">
            <%= link_to(user_path(user.id)) do %>
            <img class="user-img" src="<%= "/user_images/#{user.image_name}"%>">
            <%= user.name %>
          <% end %>
          </div>
          <div class="user-time mt-3">
            <%= l user.created_at %>
          </div>
        </div>
        </div>
      <% end %>
</div>